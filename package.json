{
    "name": "shaderbot",
    "version": "1.0.0",
    "description": "Bot of the shaderLABS Discord Server.",
    "main": "index.ts",
    "private": true,
    "scripts": {
        "start": "bun run src/index.ts",
        "update": "bun run src/update.ts",
        "migrate": "bun run src/migration/migrate.ts",
        "dev": "bun run --watch src/index.ts",
        "check": "tsc --skipLibCheck"
    },
    "author": {
        "name": "Niemand",
        "url": "https://github.com/Kneemund"
    },
    "type": "module",
    "license": "Apache-2.0",
    "licenses": [
        {
            "type": "Apache-2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0"
        }
    ],
    "dependencies": {
        "@elysiajs/cors": "1.2.0",
        "@elysiajs/jwt": "1.2.0",
        "@octokit/webhooks-methods": "5.1.0",
        "arctic": "2.3.3",
        "comment-json": "4.2.5",
        "discord.js": "14.16.3",
        "drizzle-orm": "0.38.3",
        "elysia": "1.2.9",
        "pg": "8.13.1",
        "pg-native": "3.2.0",
        "sharp": "0.33.5",
        "uuid-random": "1.3.2"
    },
    "devDependencies": {
        "@types/bun": "1.1.14",
        "@types/node-cron": "3.0.11",
        "@types/pg": "8.11.10",
        "@types/sharp": "0.32.0",
        "drizzle-kit": "0.30.1",
        "typescript": "5.7.2"
    },
    "overrides": {
        "@types/node": "^22"
    }
