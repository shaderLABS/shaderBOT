{
    "name": "shaderbot",
    "version": "1.0.0",
    "description": "Bot of the shaderLABS Discord Server.",
    "main": "index.js",
    "private": true,
    "scripts": {
        "build": "npx tsc -b --verbose",
        "start": "node -r dotenv/config build/index.js",
        "update": "node -r dotenv/config build/update.js",
        "migrate": "node -r dotenv/config build/migration/migrate.js",
        "dev": "tsc-watch --onSuccess \"node --trace-warnings -r dotenv/config build/index.js\""
    },
    "author": {
        "name": "Niemand",
        "url": "https://github.com/Kneemund"
    },
    "type": "module",
    "license": "Apache-2.0",
    "licenses": [
        {
            "type": "Apache-2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0"
        }
    ],
    "dependencies": {
        "body-parser": "^1.20.2",
        "comment-json": "^4.2.3",
        "connect-pg-simple": "^9.0.1",
        "cors": "^2.8.5",
        "discord.js": "^14.14.1",
        "express-session": "^1.17.3",
        "passport": "^0.7.0",
        "passport-discord": "^0.1.4",
        "pg": "^8.11.3",
        "polka": "^0.5.2",
        "sharp": "^0.33.2",
        "uuid-random": "^1.3.2"
    },
    "devDependencies": {
        "@types/connect-pg-simple": "^7.0.3",
        "@types/cors": "^2.8.17",
        "@types/express-session": "^1.17.10",
        "@types/node": "^20.11.8",
        "@types/node-cron": "^3.0.11",
        "@types/passport": "^1.0.16",
        "@types/passport-discord": "^0.1.11",
        "@types/pg": "^8.11.0",
        "@types/polka": "^0.5.7",
        "@types/sharp": "^0.31.1",
        "dotenv": "^16.4.1",
        "tsc-watch": "^6.0.4",
        "typescript": "^5.3.3"
    }
}
